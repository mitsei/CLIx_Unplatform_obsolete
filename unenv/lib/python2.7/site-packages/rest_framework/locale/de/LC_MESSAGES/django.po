#: unenv/lib/python2.7/site-packages/rest_framework/authentication.py:72
msgid "Invalid basic header. No credentials provided."
msgstr "Ungültiger basic header. Keine Zugangsdaten angegeben."

#: unenv/lib/python2.7/site-packages/rest_framework/authentication.py:75
msgid "Invalid basic header. Credentials string should not contain spaces."
msgstr ""
"Ungültiger basic header. Zugangsdaten sollen keine Leerzeichen enthalten."

#: unenv/lib/python2.7/site-packages/rest_framework/authentication.py:81
msgid "Invalid basic header. Credentials not correctly base64 encoded."
msgstr ""
"Ungültiger basic header. Zugangsdaten sind nicht korrekt mit base64 kodiert."

#: unenv/lib/python2.7/site-packages/rest_framework/authentication.py:98
msgid "Invalid username/password."
msgstr "Ungültiger Benutzername/Passwort"

#: unenv/lib/python2.7/site-packages/rest_framework/authentication.py:101
#: unenv/lib/python2.7/site-packages/rest_framework/authentication.py:188
msgid "User inactive or deleted."
msgstr "Benutzer inaktiv oder gelöscht."

#: unenv/lib/python2.7/site-packages/rest_framework/authentication.py:167
msgid "Invalid token header. No credentials provided."
msgstr "Ungültiger token header. Keine Zugangsdaten angegeben."

#: unenv/lib/python2.7/site-packages/rest_framework/authentication.py:170
msgid "Invalid token header. Token string should not contain spaces."
msgstr ""
"Ungültiger token header. Zugangsdaten sollen keine Leerzeichen enthalten."

#: unenv/lib/python2.7/site-packages/rest_framework/authentication.py:176
msgid ""
"Invalid token header. Token string should not contain invalid characters."
msgstr ""

#: unenv/lib/python2.7/site-packages/rest_framework/authentication.py:185
msgid "Invalid token."
msgstr "Ungültiges Token"

#: unenv/lib/python2.7/site-packages/rest_framework/authtoken/serializers.py:20
msgid "User account is disabled."
msgstr "Benutzerkonto ist gesperrt."

#: unenv/lib/python2.7/site-packages/rest_framework/authtoken/serializers.py:23
msgid "Unable to log in with provided credentials."
msgstr "Kann nicht mit den angegeben Zugangsdaten anmelden."

#: unenv/lib/python2.7/site-packages/rest_framework/authtoken/serializers.py:26
msgid "Must include \"username\" and \"password\"."
msgstr "\"username\" und \"password\" sind erforderlich."

#: unenv/lib/python2.7/site-packages/rest_framework/exceptions.py:49
msgid "A server error occurred."
msgstr "Ein Serverfehler ist aufgetreten."

#: unenv/lib/python2.7/site-packages/rest_framework/exceptions.py:84
msgid "Malformed request."
msgstr "Fehlerhafte Anfrage."

#: unenv/lib/python2.7/site-packages/rest_framework/exceptions.py:89
msgid "Incorrect authentication credentials."
msgstr "Falsche Anmeldedaten."

#: unenv/lib/python2.7/site-packages/rest_framework/exceptions.py:94
msgid "Authentication credentials were not provided."
msgstr "Anmeldedaten fehlen."

#: unenv/lib/python2.7/site-packages/rest_framework/exceptions.py:99
msgid "You do not have permission to perform this action."
msgstr "Sie sind nicht berechtigt, diese Aktion durchzuführen."

#: unenv/lib/python2.7/site-packages/rest_framework/exceptions.py:104
#: unenv/lib/python2.7/site-packages/rest_framework/views.py:81
msgid "Not found."
msgstr "Nicht gefunden."

#: unenv/lib/python2.7/site-packages/rest_framework/exceptions.py:109
#, python-brace-format
msgid "Method \"{method}\" not allowed."
msgstr "Methode \"{method}\" nicht erlaubt."

#: unenv/lib/python2.7/site-packages/rest_framework/exceptions.py:120
msgid "Could not satisfy the request Accept header."
msgstr "Kann die Accept Kopfzeile der Anfrage nicht erfüllen."

#: unenv/lib/python2.7/site-packages/rest_framework/exceptions.py:132
#, python-brace-format
msgid "Unsupported media type \"{media_type}\" in request."
msgstr "Nicht unterstützter Medientyp \"{media_type}\" in der Anfrage."

#: unenv/lib/python2.7/site-packages/rest_framework/exceptions.py:145
msgid "Request was throttled."
msgstr "Die Anfrage wurde gedrosselt."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:266
#: unenv/lib/python2.7/site-packages/rest_framework/relations.py:192
#: unenv/lib/python2.7/site-packages/rest_framework/relations.py:225
#: unenv/lib/python2.7/site-packages/rest_framework/validators.py:79
#: unenv/lib/python2.7/site-packages/rest_framework/validators.py:162
msgid "This field is required."
msgstr "Dieses Feld ist erforderlich."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:267
msgid "This field may not be null."
msgstr "Dieses Feld darf nicht Null sein."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:603
#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:634
#, python-brace-format
msgid "\"{input}\" is not a valid boolean."
msgstr "\"{input}\" ist kein gültiger Wahrheitswert."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:669
msgid "This field may not be blank."
msgstr "Dieses Feld darf nicht leer sein."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:670
#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1654
#, python-brace-format
msgid "Ensure this field has no more than {max_length} characters."
msgstr ""
"Stelle sicher, dass dieses Feld nicht mehr als {max_length} Zeichen lang ist."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:671
#, python-brace-format
msgid "Ensure this field has at least {min_length} characters."
msgstr ""
"Stelle sicher, dass dieses Feld mindestens {min_length} Zeichen lang ist."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:708
msgid "Enter a valid email address."
msgstr "Gib eine gültige E-Mail Adresse an."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:719
msgid "This value does not match the required pattern."
msgstr "Dieser Wert passt nicht zu dem erforderlichen Muster."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:730
msgid ""
"Enter a valid \"slug\" consisting of letters, numbers, underscores or "
"hyphens."
msgstr ""
"Gib ein gültiges \"slug\" aus Buchstaben, Ziffern, Unterstrichen und "
"Minuszeichen ein."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:742
msgid "Enter a valid URL."
msgstr "Gib eine gültige URL ein."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:755
#, python-brace-format
msgid "\"{value}\" is not a valid UUID."
msgstr "\"{value}\" ist keine gültige UUID."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:789
msgid "Enter a valid IPv4 or IPv6 address."
msgstr ""

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:814
msgid "A valid integer is required."
msgstr "Eine gültige Ganzzahl ist erforderlich."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:815
#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:850
#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:883
#, python-brace-format
msgid "Ensure this value is less than or equal to {max_value}."
msgstr "Stelle sicher, dass dieser Wert kleiner oder gleich {max_value} ist."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:816
#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:851
#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:884
#, python-brace-format
msgid "Ensure this value is greater than or equal to {min_value}."
msgstr "Stelle sicher, dass dieser Wert größer oder gleich {min_value} ist."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:817
#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:852
#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:888
msgid "String value too large."
msgstr "Zeichenkette zu lang."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:849
#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:882
msgid "A valid number is required."
msgstr "Eine gültige Zahl ist erforderlich."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:885
#, python-brace-format
msgid "Ensure that there are no more than {max_digits} digits in total."
msgstr ""
"Stelle sicher, dass es insgesamt nicht mehr als {max_digits} Ziffern lang "
"ist."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:886
#, python-brace-format
msgid "Ensure that there are no more than {max_decimal_places} decimal places."
msgstr ""
"Stelle sicher, dass es nicht mehr als {max_decimal_places} Nachkommastellen "
"lang ist."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:887
#, python-brace-format
msgid ""
"Ensure that there are no more than {max_whole_digits} digits before the "
"decimal point."
msgstr ""
"Stelle sicher, dass es nicht mehr als {max_whole_digits} Stellen vor dem "
"Komma lang ist."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1002
#, python-brace-format
msgid "Datetime has wrong format. Use one of these formats instead: {format}."
msgstr ""
"Datums- und Zeitangabe hat das falsche Format. Nutze stattdessen eines "
"dieser Formate: {format}."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1003
msgid "Expected a datetime but got a date."
msgstr "Erwarte eine Datums- und Zeitangabe, erhielt aber ein Datum."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1077
#, python-brace-format
msgid "Date has wrong format. Use one of these formats instead: {format}."
msgstr ""
"Datum hat das falsche Format. Nutze stattdessen eines dieser Formate: "
"{format}."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1078
msgid "Expected a date but got a datetime."
msgstr "Erwarte ein Datum, erhielt aber eine Datums- und Zeitangabe."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1146
#, python-brace-format
msgid "Time has wrong format. Use one of these formats instead: {format}."
msgstr ""
"Zeitangabe hat das falsche Format. Nutze stattdessen eines dieser Formate: "
"{format}."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1205
#, python-brace-format
msgid "Duration has wrong format. Use one of these formats instead: {format}."
msgstr ""
"Laufzeit hat das falsche Format. Benutze stattdessen eines dieser  Formate "
"{format}."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1230
#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1279
#, python-brace-format
msgid "\"{input}\" is not a valid choice."
msgstr "\"{input}\" ist keine gültige Option."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1233
#: unenv/lib/python2.7/site-packages/rest_framework/relations.py:59
#: unenv/lib/python2.7/site-packages/rest_framework/relations.py:428
#, python-brace-format
msgid "More than {count} items..."
msgstr ""

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1280
#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1427
#: unenv/lib/python2.7/site-packages/rest_framework/relations.py:424
#: unenv/lib/python2.7/site-packages/rest_framework/serializers.py:520
#, python-brace-format
msgid "Expected a list of items but got type \"{input_type}\"."
msgstr ""
"Erwarte eine Liste von Elementen, erhielt aber den Typ \"{input_type}\"."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1281
msgid "This selection may not be empty."
msgstr ""

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1318
#, python-brace-format
msgid "\"{input}\" is not a valid path choice."
msgstr ""

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1337
msgid "No file was submitted."
msgstr "Es wurde keine Datei übermittelt."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1338
msgid "The submitted data was not a file. Check the encoding type on the form."
msgstr ""
"Die übermittelten Daten stellen keine Datei dar. Prüfe den Kodierungstyp im "
"Formular."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1339
msgid "No filename could be determined."
msgstr "Der Dateiname konnte nicht ermittelt werden."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1340
msgid "The submitted file is empty."
msgstr "Die übermittelte Datei ist leer."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1341
#, python-brace-format
msgid ""
"Ensure this filename has at most {max_length} characters (it has {length})."
msgstr ""
"Stelle sicher, dass dieser Dateiname höchstens {max_length} Zeichen lang ist "
"(er hat {length})."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1389
msgid ""
"Upload a valid image. The file you uploaded was either not an image or a "
"corrupted image."
msgstr ""
"Lade ein gültiges Bild hoch. Die hochgeladene Datei ist entweder kein Bild "
"oder ein beschädigtes Bild."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1428
#: unenv/lib/python2.7/site-packages/rest_framework/relations.py:425
#: unenv/lib/python2.7/site-packages/rest_framework/serializers.py:521
msgid "This list may not be empty."
msgstr ""

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1481
#, python-brace-format
msgid "Expected a dictionary of items but got type \"{input_type}\"."
msgstr ""
"Erwarte ein Dictionary mit Elementen, erhielt aber den Typ \"{input_type}\"."

#: unenv/lib/python2.7/site-packages/rest_framework/fields.py:1528
msgid "Value must be valid JSON."
msgstr ""

