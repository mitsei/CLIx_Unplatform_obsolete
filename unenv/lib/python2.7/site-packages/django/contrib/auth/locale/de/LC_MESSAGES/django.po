msgstr "Nachname"

#: unenv/lib/python2.7/site-packages/django/contrib/auth/models.py:399
msgid "email address"
msgstr "E-Mail-Adresse"

#: unenv/lib/python2.7/site-packages/django/contrib/auth/models.py:400
msgid "staff status"
msgstr "Mitarbeiter-Status"

